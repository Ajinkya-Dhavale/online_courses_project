<%- include('navbar.ejs') %>
<div class="container mt-4">
    <div class="row">
        <div class="col-md-12 text-center">
            <h2 class="alert alert-primary">Purchase Courses List</h2>
        </div>
        <div class="col-md-12">
            <table class="table table-bordered">
                <tr>
                    <th>Sr.no</th>
                    <th>Course Name</th>
                    <th>Purchase Date</th>
                    <th>Amount</th>
                </tr>
                <% for(var i=0;i<courses_list.length;i++){ 
                %>
                    <tr>
                        <td><%= i+1 %></td>
                        <td><%= courses_list[i].course_name%></td>
                        <td><%= courses_list[i].purchase_date%></td>
                        <td><%= courses_list[i].amount%></td>
                    </tr>
                <% } %>
            </table>
        </div>
    </div>
</div>